interface su{
    abstract void sum(int m,int n);  //Abstract method.
    abstract void sub(int m,int n);
    
    
}
class math implements su{
    public void sum(int m,int n)  //defination of abstratc method
    {
        int z=m+n;
        System.out.println("The sum of the numbers are: "+ z);
    }
    public void sub(int m,int n)
    {
        int z=m-n;
        System.out.println("The differnece of the number is: "+z);
    }
}
public class Main
{
    public static void main(String args[]){
        math m=new math();
        m.sum(20,30);
        m.sub(25,15);
        
    }
}
